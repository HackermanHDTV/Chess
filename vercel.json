{
    "version": 2,
    "builds": [
        {
            "src": "server/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "server/build/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*\\..*)",
            "dest": "public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "server/index.js"
        }
    ]
}
